{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.apivcs": true,
        "**/.gitignore": true,
        "**/.designer.json": true,
        "**/.exchange.xml": true,
        "**/.vscode": true,
        "**/.mvn": true,
        "**/.tmp": true,
        "**/.settings": true,
        "**/.exchange_modules_tmp": true,
        ".mule": true
    },
    "sqltools.useNodeRuntime": true,
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "database": "${workspaceFolder:viviendo-strapi-cms}/.tmp/data.db",
            "name": "tmp"
        }
    ]
}