{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "location": {
      "type": "component",
      "repeatable": false,
      "component": "shared.location"
    },
    "catastroId": {
      "type": "string"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "features": {
      "displayName": "propertyCharacteristics",
      "type": "component",
      "repeatable": false,
      "component": "shared.property-characteristics"
    },
    "propertyStatus": {
      "type": "enumeration",
      "enum": [
        "forSale",
        "forRent",
        "onHold",
        "Rented",
        "Sold"
      ]
    },
    "listedPrice": {
      "type": "decimal"
    },
    "registrationId": {
      "type": "string",
      "required": true
    },
    "test": {
      "type": "string"
    },
    "propertyType": {
      "type": "enumeration",
      "enum": [
        "singleFamilyHome",
        "condo",
        "loft",
        "plex",
        "intergeneration",
        "mobileHome",
        "farm",
        "cottage",
        "land"
      ],
      "default": "singleFamilyHome"
    },
    "building": {
      "displayName": "building",
      "type": "component",
      "repeatable": false,
      "component": "shared.building"
    }
  }
}
